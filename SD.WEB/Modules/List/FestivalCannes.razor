@page "/List/FestivalCannes"
<PageTitle>SD - Lists - Festival de Cannes</PageTitle>
@using SD.WEB.Modules.List.Core
@using SD.WEB.Modules.List.Components
@inject TmdbListApi TmdbListApi

<HeadPage ImageSource="logo/festival-cannes.png" Text="All the news from the 1st film festival in the world." Link="https://www.festival-cannes.com/en/"></HeadPage>

<MediaList MediaListApi="TmdbListApi" List="@EnumLists.FestivalCannesPalme" MaxItens="10"></MediaList>
<MediaList MediaListApi="TmdbListApi" List="@EnumLists.FestivalCannesGrandPrix" MaxItens="10"></MediaList>
<MediaList MediaListApi="TmdbListApi" List="@EnumLists.FestivalCannesPalmeShortFilm" MaxItens="10"></MediaList>