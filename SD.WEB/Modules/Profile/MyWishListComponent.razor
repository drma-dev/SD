@using SD.WEB.Modules.Profile.Components

<Wishlist MediaType="MediaType.movie" Private="true" ShowHeader="true" FullScreen="false" WatchedList="WatchedList" WishList="WishList"
          Core="CoreWishlist" ExpressionEmpty="(WishList? list) => list == null || list.Movies.Empty()"></Wishlist>

<Wishlist MediaType="MediaType.tv" Private="true" ShowHeader="true" FullScreen="false" WatchedList="WatchedList" WishList="WishList"
          Core="CoreWishlist" ExpressionEmpty="(WishList? list) => list == null || list.Shows.Empty()"></Wishlist>

@code {
    [Parameter][EditorRequired] public RenderControlCore<WishList?> CoreWishlist { get; set; } = new();
    [Parameter][EditorRequired] public WatchedList? WatchedList { get; set; }
    [Parameter][EditorRequired] public WishList? WishList { get; set; }
}