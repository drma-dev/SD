{
  "routes": [
    {
      "route": "/api/public/*",
      "allowedRoles": [ "anonymous" ]
    },
    {
      "route": "/api/adm/*",
      "allowedRoles": [ "administrator" ]
    },
    {
      "route": "/api/*",
      "allowedRoles": [ "authenticated" ]
    },
    {
      "route": "/ads.txt",
      "redirect": "https://srv.adstxtmanager.com/19390/streamingdiscovery.com",
      "statusCode": 301
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [ "*.{css,scss,js,png,gif,ico,jpg,svg}" ]
  }
}