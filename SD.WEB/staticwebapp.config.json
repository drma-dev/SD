{
  "routes": [
    {
      "route": "/api/public/*",
      "allowedRoles": [
        "anonymous"
      ]
    },
    {
      "route": "/api/adm/*",
      "allowedRoles": [
        "administrator"
      ]
    },
    {
      "route": "/api/*",
      "allowedRoles": [
        "authenticated"
      ]
    },
    {
      "route": "https://dev.streamingdiscovery.com/*",
      "rewrite": "/index.html"
    },
    {
      "route": "/*",
      "redirect": "https://streamingdiscovery.com/:splat",
      "statusCode": 301
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "*.{css,scss,js,png,gif,ico,jpg,svg}"
    ]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/404"
    }
  }
}