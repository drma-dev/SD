{
  "routes": [
    {
      "route": "/api/public/*",
      "allowedRoles": [
        "anonymous"
      ]
    },
    {
      "route": "/api/adm/*",
      "allowedRoles": [
        "administrator"
      ]
    },
    {
      "route": "/api/*",
      "allowedRoles": [
        "authenticated"
      ]
    },
    {
      "route": "/*",
      "redirect": "https://www.streamingdiscovery.com/",
      "statusCode": 301
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "*.{css,scss,js,png,gif,ico,jpg,svg,ttf,woff2,webmanifest}"
    ]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/404.html",
      "statusCode": 404
    }
  }
}